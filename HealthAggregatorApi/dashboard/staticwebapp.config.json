{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/css/*", "/js/*", "*.{css,js,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot}"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Cache-Control": "max-age=300"
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
