{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "*.{css,js,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot}"]
  },
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://func-healthaggregator.azurewebsites.net/api/*"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Cache-Control": "max-age=300"
  }
}
